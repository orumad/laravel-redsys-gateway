{
    "name": "orumad/laravel-redsys-gateway",
    "description": "Laravel package to integrate the spanish Redsys payment gateway",
    "keywords": [
        "orumad",
        "redsys",
        "redsys gateway",
        "payment gateway",
        "tpv",
        "laravel package",
        "laravel"
    ],
    "homepage": "https://github.com/orumad/laravel-spanish-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mu√±oz",
            "email": "dev@danielmunoz.io",
            "homepage": "https://danielmunoz.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "spatie/ray": "^1.21"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "nunomaduro/collision": "^5.4",
        "orchestra/testbench": "^6.17",
        "pestphp/pest": "^1.0",
        "phpunit/phpunit": "^9.5",
        "scrutinizer/ocular": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Orumad\\LaravelRedsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orumad\\LaravelRedsys\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orumad\\LaravelRedsys\\LaravelRedsysServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
